{
    "runOnSave.statusMessageTimeout": 3000,
    "runOnSave.commands": [
        {
            "match": ".*\\.tex$",
            "command": "make file=${file}",
            "runIn": "backend",
            "runningStatusMessage": "Compiling ${fileBasename}...",
            "finishStatusMessage": "${fileBasename} compiled!"
        },
    ],
    "cSpell.words": [
        "anytrading",
        "autoencoder",
        "cuda",
        "LLDQN",
        "logits",
        "logspace",
        "Offpolicy",
        "probs",
        "Softmax",
        "tensorboard",
        "tianshou",
        "wandb"
    ],
    "python.formatting.provider": "black"
}
